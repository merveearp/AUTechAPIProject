@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">

    @await Component.InvokeAsync("_LayoutHeadComponent")


<body>
    <!-- Spinner Start -->
    <div id="spinner"
         class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
        <div class="spinner-grow text-primary" style="width: 3rem; height: 3rem;" role="status">
            <span class="sr-only">Loading...</span>
        </div>
    </div>
    <!-- Spinner End -->
    <!-- Navbar Start -->
    @await Component.InvokeAsync("_LayoutNavbarComponent")
   
    <!-- Navbar End -->

    @RenderBody()
    
    <!-- Footer Start -->
    @await Component.InvokeAsync("_LayoutFooterComponent")
   
    <!-- Footer End -->
   
    <!-- Back to Top -->
  
    <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top pt-2"><i class="bi bi-arrow-up"></i></a>

    <!-- JavaScript Libraries -->
    @await Component.InvokeAsync("_LayoutScriptsComponent")
    

    <!-- Template Javascript -->
    <script src="~/WEBUI/js/main.js"></script>
</body>

</html>